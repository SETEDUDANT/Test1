<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp"
    android:background="#01DA58">



    <TextView
        android:id="@+id/textView"
        android:textColor="@android:color/white"
        android:textStyle="bold"
        android:layout_width="293dp"
        android:layout_height="53dp"
        android:textSize="17sp"
        android:text="BEFORE YOU SAVE, PLEASE TELL US A BIT MORE ABOUT YOURSELF  :-)"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintHorizontal_bias="0.509" />


    <EditText
        android:id="@+id/eTname"
        android:layout_width="293dp"
        android:layout_height="30dp"
        android:background="@drawable/personalinfo"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView" />

    <EditText
        android:id="@+id/eTroomNumber"
        android:layout_width="293dp"
        android:layout_height="30dp"
        android:background="@drawable/personalinfo"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView8"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />" />

    <EditText
        android:id="@+id/eTeMail"
        android:layout_width="293dp"
        android:layout_height="30dp"
        android:background="@drawable/personalinfo"
        android:layout_marginTop="-1dp"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView" />

    <TextView
        android:id="@+id/textView2"
        android:textColor="@android:color/white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="(NICK) NAME: "
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/eTname"
        app:layout_constraintTop_toTopOf="@+id/textView10"
        android:layout_marginTop="14dp" />

    <TextView
        android:id="@+id/textView3"
        android:textColor="@android:color/white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="9dp"
        android:textStyle="bold"
        android:text=" (WILL BE SHOWN "
        android:layout_marginTop="3dp"
        app:layout_constraintTop_toTopOf="@+id/textView2"
        app:layout_constraintLeft_toRightOf="@+id/textView2"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp" />

    <TextView
        android:id="@+id/textView4"
        android:textColor="@android:color/white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="E_MAIL:"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/eTeMail"
        android:layout_marginTop="13dp"
        app:layout_constraintTop_toBottomOf="@+id/eTname" />

    <TextView
        android:id="@+id/textView5"
        android:textColor="@android:color/white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="(WILL STAY PRIVATE"
        android:textSize="9dp"
        android:textStyle="bold"
        android:layout_marginTop="3dp"
        app:layout_constraintLeft_toRightOf="@+id/textView4"
        android:layout_marginLeft="8dp"
        app:layout_constraintTop_toTopOf="@+id/textView4"
        android:layout_marginStart="8dp" />

    <TextView
        android:id="@+id/textView6"
        android:textColor="@android:color/white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SO NO ONE WILL SEE YOUR E-MAIL)"
        android:textSize="9dp"
        app:layout_constraintLeft_toRightOf="@+id/textView5"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="3dp"
        app:layout_constraintTop_toTopOf="@+id/textView4"
        android:layout_marginStart="2dp" />

    <TextView
        android:id="@+id/textView7"
        android:textColor="@android:color/white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="WITH YOUR SEEVIEWS)"
        android:textSize="9dp"
        android:layout_marginTop="3dp"
        app:layout_constraintTop_toTopOf="@+id/textView2"
        app:layout_constraintLeft_toRightOf="@+id/textView3"
        android:layout_marginLeft="2dp"
        android:layout_marginStart="2dp" />

    <Button
        android:id="@+id/bt20"
        android:layout_width="62dp"
        android:layout_height="40dp"
        android:background="#ffffff"
        android:text="20"
        android:textColor="#01DA58"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <Button
        android:id="@+id/bt2130"
        android:layout_width="62dp"
        android:layout_height="40dp"
        android:layout_marginLeft="15dp"
        android:layout_marginStart="15dp"
        android:background="#ffffff"
        android:text="21/30"
        android:textColor="#01DA58"
        app:layout_constraintLeft_toRightOf="@+id/bt20"
        app:layout_constraintTop_toTopOf="@+id/bt20" />

    <Button
        android:id="@+id/bt3140"
        android:layout_width="62dp"
        android:layout_height="40dp"
        android:layout_marginLeft="15dp"
        android:layout_marginStart="15dp"
        android:background="#ffffff"
        android:text="31/40"
        android:textColor="#01DA58"
        app:layout_constraintLeft_toRightOf="@+id/bt2130"
        app:layout_constraintTop_toTopOf="@+id/bt20" />

    <Button
        android:id="@+id/bt4150"
        android:layout_width="62dp"
        android:layout_height="40dp"
        android:layout_marginLeft="15dp"
        android:layout_marginStart="15dp"
        android:background="#ffffff"
        android:text="41/50"
        android:textColor="#01DA58"
        app:layout_constraintLeft_toRightOf="@+id/bt3140"
        app:layout_constraintTop_toTopOf="@+id/bt20" />

    <Button
        android:id="@+id/bt5160"
        android:layout_width="62dp"
        android:layout_height="40dp"
        android:background="#ffffff"
        android:text="51/60"
        android:textColor="#01DA58"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/bt20"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/bt20" />

    <Button
        android:id="@+id/bt6170"
        android:layout_width="62dp"
        android:layout_height="40dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="15dp"
        android:background="#ffffff"
        android:text="61/70"
        android:textColor="#01DA58"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/bt5160"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/bt5160" />

    <TextView
        android:id="@+id/textView13"
        android:textColor="@android:color/white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="AGE:"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/eTeMail" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="9dp"
        android:layout_marginTop="3dp"
        android:text="(WILL BE SHOWN"
        android:textColor="@android:color/white"
        android:textSize="9dp"
        android:textStyle="bold"
        app:layout_constraintLeft_toRightOf="@+id/textView13"
        app:layout_constraintTop_toTopOf="@+id/textView13"
        android:layout_marginStart="9dp" />

    <TextView
        android:id="@+id/textView15"
        android:layout_marginTop="3dp"
        android:textSize="9dp"
        android:textColor="@android:color/white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="WITH YOUR SEEVIEWS)"
        app:layout_constraintLeft_toRightOf="@+id/textView14"
        android:layout_marginLeft="2dp"
        app:layout_constraintTop_toTopOf="@+id/textView13"
        android:layout_marginStart="2dp" />

    <TextView
        android:id="@+id/textView8"
        android:textColor="@android:color/white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="YOUR ROOM NUMBER:"
        app:layout_constraintLeft_toLeftOf="@+id/textView"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/bt5160" />

    <Spinner
        android:id="@+id/spinner4"
        android:spinnerMode="dialog"
        android:layout_width="293dp"
        style="@android:style/Widget.Holo.Light.Spinner"
        android:layout_height="33dp"
        android:layout_marginTop="8dp"
        android:background="#FFFFFF"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView" />

    <TextView
        android:id="@+id/textView9"
        android:textColor="@android:color/white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LANGUAGE:"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/eTroomNumber" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="293dp"
        android:layout_height="1dp"
        android:background="@android:color/white"
        app:layout_constraintLeft_toLeftOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent">

        <Button
            android:id="@+id/button5"
            android:layout_width="0dp"
            android:background="#EE4A17"
            android:textColor="#FFFFFF"
            android:layout_weight="6"
            android:layout_height="wrap_content"
            android:src="@android:drawable/ic_menu_search"
            android:text="SAVE YOUR DETAILS AND GO!" />

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/bak"
            android:scaleType="fitXY"/>

    </LinearLayout>
    android:text="TextView"
    tools:layout_editor_absoluteX="46dp"
    tools:layout_editor_absoluteY="69dp" />
</android.support.constraint.ConstraintLayout>
